{
  "properties": {
    "amdPackages": "apprt@^3.8.0"
  },
  "load": {
    "bundleLocations": [
      "localbundles",
      "bundles"
    ],
    "allowedBundles": [
      "system@3.x",
      "splashscreen@3.x",
      "map@3.x",
      "themes@3.x",
      "templates@3.x",
      "templatelayout@3.x",
      "windowmanager@3.x",
      "toolset@3.x",
      "toolrules@3.x",
      "basemaptoggler@3.x",
      "sample_helloworld"
    ],
    "skipBundles": [],
    "require": [
      "${app}.app"
    ],
    "i18n": [
      "bundle"
    ]
  },
  "bundles": {
    "map": {
      "MapState": {
        "initialExtent": {
          "xmin": -1100000,
          "ymin": 4000000,
          "xmax": 3500000,
          "ymax": 8800000,
          "spatialReference": {
            "wkid": 3857
          }
        }
      },
      "MapModelFactory": {
        "_configData": {
          "maps": [
            {
              "id": "default",
              "glass_pane": [],
              "operationalLayer": [],
              "baseLayer": [
                {
                  "title": "${service.streets}",
                  "service": "streets",
                  "enabled": true
                },
                {
                  "title": "${service.topo}",
                  "service": "worldTopo",
                  "enabled": false
                }
              ]
            }
          ]
        }
      },
      "MappingResourceRegistryFactory": {
        "_knownServices": {
          "services": [
            {
              "id": "worldTopo",
              "type": "AGS_TILED",
              "url": "http://services.arcgisonline.com/ArcGIS/rest/services/World_Topo_Map/MapServer"
            },
            {
              "id": "streets",
              "type": "AGS_TILED",
              "url": "http://services.arcgisonline.com/ArcGIS/rest/services/World_Street_Map/MapServer"
            }
          ]
        }
      }
    }
  }
}
